<!doctype html>
<html class="no-js h-100" lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>@ViewBag.Title - Hornos Roberto Navarro</title>
    <meta name="description" content="Sistema Integral de Hornos Roberto Navarro">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="~/Vendor/font-awesome-5.11.2/css/all.css" rel="stylesheet" />

    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

    <link href="~/Vendor/bootstrap-4.3.1-dist/css/bootstrap.min.css" rel="stylesheet" />

    <link rel="stylesheet" href="~/Content/extras.1.1.0.min.css">
    <link rel="stylesheet" href="~/Content/shards-dashboards.1.1.0.min.css">
    <link rel="icon" type="image/jpeg" href="~/Resources/logo claro HRN.jpeg" />

    <script src="~/Vendor/github-buttons/dist/buttons.js"></script>

    <link href="~/Resources/hrn.css" rel="stylesheet" />



    <link href="~/Vendor/DataTables/DataTables-1.10.20/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="~/Vendor/DataTables/AutoFill-2.3.4/css/autoFill.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/Vendor/DataTables/Buttons-1.6.1/css/buttons.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/Vendor/DataTables/ColReorder-1.5.2/css/colReorder.dataTables.min.css" rel="stylesheet" />
    <link href="~/Vendor/DataTables/FixedColumns-3.3.0/css/fixedColumns.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/Vendor/DataTables/FixedHeader-3.1.6/css/fixedHeader.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/Vendor/DataTables/KeyTable-2.5.1/css/keyTable.bootstrap4.css" rel="stylesheet" />
    <link href="~/Vendor/DataTables/Responsive-2.2.3/css/responsive.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/Vendor/DataTables/RowGroup-1.1.1/css/rowGroup.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/Vendor/DataTables/RowReorder-1.2.6/css/rowReorder.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/Vendor/DataTables/Scroller-2.0.1/css/scroller.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/Vendor/DataTables/Select-1.3.1/css/select.bootstrap4.min.css" rel="stylesheet" />

    




</head>
<body class="h-100">
    <div class="container-fluid">
        <div class="row">
            <!-- Main Sidebar -->
            <aside class="main-sidebar col-12 col-md-3 col-lg-2 px-0">
                <div class="main-navbar">
                    <nav class="navbar align-items-stretch navbar-light bg-white flex-md-nowrap border-bottom p-0">
                        <a class="navbar-brand w-100 mr-0" href="#" style="line-height: 25px;">
                            <div class="d-table m-auto">
                                <img id="main-logo" class="d-inline-block align-top mr-1" style="max-width: 40px;" src="~/Resources/logo claro HRN.jpeg">
                                <span class="d-none d-md-inline ml-1">Hornos Roberto Navarro</span>
                            </div>
                        </a>
                        <a class="toggle-sidebar d-sm-inline d-md-none d-lg-none">
                            <i class="material-icons">&#xE5C4;</i>
                        </a>
                    </nav>
                </div>
                <div class="nav-wrapper">
                    <ul class="nav flex-column">
                        <li class="nav-item">
                            <a class="nav-link " href="http://localhost:54674/Clients">
                                <img style="max-height:15px;max-width:15px;" src="~/Resources/clientes.ico">
                                <span>Clientes</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link " href="http://localhost:54674/Products">
                                <img style="max-height:15px;max-width:15px;" src="~/Resources/productos.svg">
                                <span>Productos</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="http://localhost:54674/Sales">
                                <img style="max-height:15px;max-width:15px;" src="~/Resources/facturacion.png">
                                <span>Facturación</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link " href="http://localhost:54674/Users">
                                <i class="material-icons">person</i>
                                <span>Empleados</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link " href="http://localhost:54674/Proforma/Create">
                                <i class="material-icons">note_add</i>
                                <span>Proformas</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link " href="http://localhost:54674/Earnings">
                                <i class="material-icons">assessment</i>
                                <span>Beneficio Económico</span>
                            </a>
                        </li>
                    </ul>
                </div>
            </aside>
            <!-- End Main Sidebar -->
            <main class="main-content col-lg-10 col-md-9 col-sm-12 p-0 offset-lg-2 offset-md-3">
                <div class="main-navbar sticky-top bg-white">
                    <!-- Main Navbar -->
                    <!-- Espacio Blanco en la barra de arriba -->
                    <nav class="navbar align-items-stretch navbar-light flex-md-nowrap p-0">
                        <form action="#" class="main-navbar__search w-100 d-none d-md-flex d-lg-flex">
                            <div class="input-group input-group-seamless ml-3">
                                <div class="input-group-prepend">
                                    <div class="input-group-text">

                                    </div>
                                </div>
                            </div>
                        </form>
                        <ul class="navbar-nav border-left flex-row ">
                            <li class="nav-item border-right dropdown notifications">

                            </li>
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle text-nowrap px-3" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">
                                    <i class="material-icons">person</i>
                                    <span class="d-none d-md-inline-block">@Session["userName2"]</span>
                                </a>
                                <div class="dropdown-menu dropdown-menu-small">
                                    <a class="dropdown-item text-danger" href="http://localhost:54674/Sales/CerrarSesion">
                                        <i class="material-icons text-danger">&#xE879;</i>Cerrar Sesión
                                    </a>
                                </div>
                            </li>
                        </ul>
                    </nav>
                </div>
                <!-- / .main-navbar -->
                <div class="main-content-container container-fluid px-4">
                    <!-- Page Header -->
                    <br />
                    <!-- End Page Header -->
                    <!-- Default Light Table -->
                    <div class="row">
                        <div class="col">
                            <div class="card card-small mb-4">
                                <div class="card-body container-fluid">
                                    @RenderBody()
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- End Default Light Table -->
                </div>
                <footer class="main-footer d-flex p-2 px-3 bg-white border-top">
                    <div class="col-4"></div>
                    <span class="copyright col-4">
                        Copyright © 2019
                        <a href="#" rel="nofollow">Hornos Roberto Navarro</a>
                    </span>
                    <div class="col-4"></div>
                </footer>
            </main>
        </div>
    </div>

    <script src="~/Vendor/jquery-3.3.1/jquery-3.3.1.min.js"></script>


    <script src="~/Vendor/popper-1.14.3/popper.min.js"></script>
    <script src="~/Vendor/bootstrap-4.3.1-dist/js/bootstrap.min.js"></script>
    <script src="~/Vendor/chartjs-2.9.2/Chart.js"></script>

    <script src="https://unpkg.com/shards-ui@latest/dist/js/shards.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Sharrre/2.0.1/jquery.sharrre.min.js"></script>
    <script src="~/scripts/extras.1.1.0.min.js"></script>
    <script src="~/scripts/shards-dashboards.1.1.0.min.js"></script>


    <script src="~/Vendor/DataTables/JSZip-2.5.0/jszip.min.js"></script>
    <script src="~/Vendor/DataTables/pdfmake-0.1.36/pdfmake.min.js"></script>
    <script src="~/Vendor/DataTables/pdfmake-0.1.36/vfs_fonts.js"></script>
    <script src="~/Vendor/DataTables/DataTables-1.10.20/js/jquery.dataTables.min.js"></script>
    <script src="~/Vendor/DataTables/DataTables-1.10.20/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/Vendor/DataTables/AutoFill-2.3.4/js/dataTables.autoFill.min.js"></script>
    <script src="~/Vendor/DataTables/AutoFill-2.3.4/js/autoFill.bootstrap4.js"></script>
    <script src="~/Vendor/DataTables/Buttons-1.6.1/js/dataTables.buttons.min.js"></script>
    <script src="~/Vendor/DataTables/Buttons-1.6.1/js/buttons.bootstrap4.min.js"></script>
    <script src="~/Vendor/DataTables/Buttons-1.6.1/js/buttons.colVis.min.js"></script>
    <script src="~/Vendor/DataTables/Buttons-1.6.1/js/buttons.flash.min.js"></script>
    <script src="~/Vendor/DataTables/Buttons-1.6.1/js/buttons.html5.min.js"></script>
    <script src="~/Vendor/DataTables/Buttons-1.6.1/js/buttons.print.min.js"></script>
    <script src="~/Vendor/DataTables/ColReorder-1.5.2/js/dataTables.colReorder.min.js"></script>
    <script src="~/Vendor/DataTables/FixedColumns-3.3.0/js/dataTables.fixedColumns.min.js"></script>
    <script src="~/Vendor/DataTables/FixedHeader-3.1.6/js/dataTables.fixedHeader.min.js"></script>
    <script src="~/Vendor/DataTables/KeyTable-2.5.1/js/dataTables.keyTable.min.js"></script>
    <script src="~/Vendor/DataTables/Responsive-2.2.3/js/dataTables.responsive.min.js"></script>
    <script src="~/Vendor/DataTables/Responsive-2.2.3/js/responsive.bootstrap4.min.js"></script>
    <script src="~/Vendor/DataTables/RowGroup-1.1.1/js/dataTables.rowGroup.min.js"></script>
    <script src="~/Vendor/DataTables/RowReorder-1.2.6/js/dataTables.rowReorder.min.js"></script>
    <script src="~/Vendor/DataTables/Scroller-2.0.1/js/dataTables.scroller.min.js"></script>
    <script src="~/Vendor/DataTables/Select-1.3.1/js/dataTables.select.min.js"></script>


    @RenderSection("scripts", required: false)
</body>
</html>